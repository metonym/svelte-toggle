{
  "name": "svelte-toggle",
  "version": "4.0.0",
  "license": "MIT",
  "description": "Accessible toggle switch component",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./src/index.js",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "svelte": "./src/index.js",
      "default": "./src/index.js"
    },
    "./src/*.svelte": {
      "types": "./src/*.svelte.d.ts",
      "import": "./src/*.svelte"
    },
    "./src/*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "scripts": {
    "dev": "rollup -cw",
    "predeploy": "rollup -c",
    "deploy": "gh-pages -d dist",
    "check": "svelte-check --workspace tests",
    "test": "vitest",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@testing-library/svelte": "^3.2.2",
    "gh-pages": "^5.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-readme": "^3.6.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.31.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-toggle.git"
  },
  "homepage": "https://github.com/metonym/svelte-toggle",
  "bugs": "https://github.com/metonym/svelte-toggle/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "toggle",
    "switch",
    "button",
    "accessible",
    "a11y",
    "wai-aria"
  ],
  "files": [
    "src"
  ],
  "type": "module"
}
