{
  "name": "svelte-toggle",
  "version": "4.0.1",
  "license": "MIT",
  "description": "Accessible toggle switch component",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./src/index.js",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "svelte": "./src/index.js",
      "default": "./src/index.js"
    },
    "./src/*.svelte": {
      "types": "./src/*.svelte.d.ts",
      "import": "./src/*.svelte"
    },
    "./src/*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "scripts": {
    "dev": "rollup -cw",
    "check": "svelte-check --workspace tests",
    "test": "vitest",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/svelte": "^4.1.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "svelte-readme": "^3.6.3",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-toggle.git"
  },
  "homepage": "https://github.com/metonym/svelte-toggle",
  "bugs": "https://github.com/metonym/svelte-toggle/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "toggle",
    "switch",
    "button",
    "accessible",
    "a11y",
    "wai-aria"
  ],
  "files": [
    "src"
  ],
  "type": "module"
}
